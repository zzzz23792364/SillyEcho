# cross compiler variable

#CROSS_COMPILE := arm-linux-
CROSS_COMPILE := 

CC := $(CROSS_COMPILE)gcc
AR := $(CROSS_COMPILE)ar
CXX := $(CROSS_COMPILE)g++
STRIP := $(CROSS_COMPILE)strip
OBJCOPY := $(CROSS_COMPILE)objcopy

# common shell command
CURR_DIR = $(shell pwd)
MAKE := make
CP := cp
ECHO := echo
MKDIR := mkdir
RM := rm -f


